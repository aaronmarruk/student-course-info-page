{
    "name":"revuls/slimMVC",
    "type":"framework",
    "description":"MVC for the PHP microframework Slim",
    "keywords":["microframework", "rest", "router", "mvc"],
    "homepage":"https://github.com/revuls/SlimMVC",
    "license":"MIT",
    "authors":[
        {
            "name":"revuls"
        }
    ],
    "require":{
        "php":">=5.3.0",
        "oodle/krumo": "*",
        "slim/slim":"2.*",
        "slim/views":"0.1.*",
        "twig/twig":"1.*",
        "symfony/yaml": "2.6.*@dev"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "maegnes/SlimCachingManager",
                "version": "*",
                "dist": {
                    "url": "https://github.com/maegnes/SlimCachingManager/",
                    "type": "vcs"
                },
                "autoload": {
                    "psr-0": { "maegnes\\SlimCachingManager": "" }
                },
                "target-dir": "maegnes/SlimCachingManager"
            }
        }
    ],
    "autoload":{
        "psr-0":{
            "": ""
        }
    }
}